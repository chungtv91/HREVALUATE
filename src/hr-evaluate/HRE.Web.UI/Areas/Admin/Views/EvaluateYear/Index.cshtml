@model IEnumerable<HRE.Core.Entities.EvaluateYear>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FromYear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ToYear)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FromYear)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ToYear)
        </td>
        <td>
